{
  "name": "modo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0 --disable-host-check",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run modo:server:production",
    "build:ssr:functions": "npm run build:ssr && cp -r dist/ functions/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~17.0.9",
    "@angular/cdk": "~17.0.5",
    "@angular/common": "~17.0.9",
    "@angular/compiler": "~17.0.9",
    "@angular/core": "~17.0.9",
    "@angular/fire": "^17.0.1",
    "@angular/forms": "~17.0.9",
    "@angular/material": "^17.0.5",
    "@angular/platform-browser": "~17.0.9",
    "@angular/platform-browser-dynamic": "~17.0.9",
    "@angular/platform-server": "~17.0.9",
    "@angular/pwa": "^17.0.10",
    "@angular/router": "~17.0.9",
    "@angular/service-worker": "~17.0.9",
    "@angular/ssr": "17.0.10",
    "core-js": "^2.5.4",
    "express": "^4.17.3",
    "firebase": "^10.7.1",
    "hammerjs": "^2.0.8",
    "rxjs": "~7.8.1",
    "tslib": "^1.9.0",
    "xmlhttprequest": "^1.8.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~17.0.10",
    "@angular/cli": "~17.0.10",
    "@angular/compiler-cli": "~17.0.9",
    "@angular/language-service": "~17.0.9",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~7.0.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~5.2.0",
    "webpack-cli": "^3.1.0"
